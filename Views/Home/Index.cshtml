<section class="py-0">
    <div class="container">
        <div class="row h-100">

            @*<div class="form-group">
            <label asp-for="govarnateId" class="control-label"></label>
            <select asp-for="govarnateId" class ="form-control" asp-items="ViewBag.govarnateId"></select>
            </div>
            <div class="form-group">
            <label asp-for="regionId" class="control-label"></label>
            <select asp-for="regionId" class ="form-control" asp-items="ViewBag.regionId"></select>
            </div>*@

            <div class="form-group col-md-4">
                <label class="control-label"> محافظة</label>

                <select id="govarnateId" class="form-control" asp-items="@ViewBag.govarnateId"></select>

            </div>

            <div class="form-group col-md-4">
                <label class="control-label">منطقة</label>

                <select id="regionId" class="form-control" asp-items="@ViewBag.regionId"></select>

            </div>

            <div class="form-group col-md-2" style="margin-top:30px">

                <button id="btnSearch" class="btn btn-success">بحث</button>

            </div>



            <div class="col-lg-7 mx-auto text-center mb-6">
                <h5 class="fs-3 fs-lg-5 lh-sm mb-3">الاحدث اضافة</h5>
            </div>
            <div class="col-12">
                <div class="carousel slide" id="carouselNewArrivals" data-bs-ride="carousel">
                    <div class="carousel-inner">
                        <div class="carousel-item active" data-bs-interval="10000">
                            <div class="row h-100 align-items-center g-2">
                                @if (Model != null && Model.Count() > 0)
                                {
                                    @for (int i = 0; i < 4; i++)
                                    {
                                        @if (Model.Count() > i)
                                        {
                                            <div class="col-sm-6 col-md-3 mb-3 mb-md-0 h-100" style="height: 400px !important;">
                                                <div class="card card-span h-100 text-white">

                                                    <img class="card-img h-100" src="@Model[i]?.Images?.ToList()[0].path.Split("wwwroot")[1]" alt="..." />
                                                    <div class="card-img-overlay bg-dark-gradient d-flex flex-column-reverse">
                                                        <h6 class="text-primary">@Model[i].Price</h6>
                                                        @*<p class="text-400 fs-1">@Model[i].regionId</p>*@
                                                        <h4 class="text-light">@Model[i].email</h4>
                                                    </div>
                                                    <a target="_blank" class="stretched-link" href="@Url.Action("details","posts",new{id=Model[i].id})"></a>
                                                </div>


                                            </div>
                                        }
                                    }
                                }

                            </div>
                        </div>
                        @if (Model != null && Model.Count() > 4)
                        {
                            <div class="carousel-item" data-bs-interval="10000">
                                <div class="row h-100 align-items-center g-2">
                                    @for (int i = 4; i < 8; i++)
                                    {
                                        @if (Model.Count() > i)
                                        {
                                            <div class="col-sm-6 col-md-3 mb-3 mb-md-0 h-100" style="height: 400px !important;">
                                                <div class="card card-span h-100 text-white">

                                                    <img class="card-img h-100" src="@Model[i]?.Images?.ToList()[0].path.Split("wwwroot")[1]" alt="..." />
                                                    <div class="card-img-overlay bg-dark-gradient d-flex flex-column-reverse">
                                                        <h6 class="text-primary">@Model[i].Price</h6>
                                                        @*<p class="text-400 fs-1">@Model[i].Region</p>*@
                                                        <h4 class="text-light">@Model[i].email</h4>
                                                    </div><a class="stretched-link" href="#"></a>
                                                </div>
                                            </div>
                                        }

                                    }
                                </div>
                            </div>

                        }

                        @if (Model != null && Model.Count() > 8)
                        {
                            <div class="carousel-item" data-bs-interval="10000">
                                <div class="row h-100 align-items-center g-2">
                                    @for (int i = 8; i < 12; i++)
                                    {
                                        @if (Model.Count() > i)
                                        {
                                            <div class="col-sm-6 col-md-3 mb-3 mb-md-0 h-100" style="height: 400px !important;">
                                                <div class="card card-span h-100 text-white">

                                                    <img class="card-img h-100" src="@Model[i]?.Images?.ToList()[0].path.Split("wwwroot")[1]" alt="..." />
                                                    <div class="card-img-overlay bg-dark-gradient d-flex flex-column-reverse">
                                                        <h6 class="text-primary">@Model[i].Price</h6>
                                                        @*<p class="text-400 fs-1">@Model[i].Region</p>*@
                                                        <h4 class="text-light">@Model[i].email</h4>
                                                    </div><a class="stretched-link" href="#"></a>
                                                </div>
                                            </div>
                                        }

                                    }
                                </div>
                            </div>

                        }
                        <div class="row">
                            <button class="carousel-control-prev" type="button" data-bs-target="#carouselNewArrivals" data-bs-slide="prev"><span class="carousel-control-prev-icon" aria-hidden="true"></span><span class="visually-hidden">Previous</span></button>
                            <button class="carousel-control-next" type="button" data-bs-target="#carouselNewArrivals" data-bs-slide="next"><span class="carousel-control-next-icon" aria-hidden="true"></span><span class="visually-hidden">Next </span></button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
